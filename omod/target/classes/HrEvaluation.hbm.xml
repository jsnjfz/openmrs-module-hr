<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.openmrs.module.hr.HrEvaluation" table="hr_evaluation" catalog="openmrs">
        <id name="evaluationId" type="int">
            <column name="evaluation_id" />
            <generator class="assigned" />
        </id>
        <many-to-one name="hrStaff" class="org.openmrs.module.hr.HrStaff" fetch="select">
            <column name="staff_id" not-null="true" />
        </many-to-one>
        <many-to-one name="hrCompetency" class="org.openmrs.module.hr.HrCompetency" fetch="select">
            <column name="competency_id" not-null="true" />
        </many-to-one>
        <property name="evaluatorId" type="int">
            <column name="evaluator_id" not-null="true" />
        </property>
        <property name="level" type="string">
            <column name="level" length="50" not-null="true" />
        </property>
        <property name="evaluationDate" type="timestamp">
            <column name="evaluation_date" length="19" not-null="true" />
        </property>
        <property name="creator" type="int">
            <column name="creator" not-null="true" />
        </property>
        <property name="dateCreated" type="timestamp">
            <column name="date_created" length="19" not-null="true" />
        </property>
        <property name="changedBy" type="java.lang.Integer">
            <column name="changed_by" />
        </property>
        <property name="dateChanged" type="timestamp">
            <column name="date_changed" length="19" />
        </property>
        <property name="voided" type="short">
            <column name="voided" not-null="true" />
        </property>
        <property name="voidedBy" type="java.lang.Integer">
            <column name="voided_by" />
        </property>
        <property name="dateVoided" type="timestamp">
            <column name="date_voided" length="19" />
        </property>
        <property name="voidReason" type="string">
            <column name="void_reason" />
        </property>
        <property name="uuid" type="string">
            <column name="uuid" length="38" not-null="true" unique="true" />
        </property>
    </class>
</hibernate-mapping>
