<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.openmrs.module.hr.HrStaffAttributeType" table="hr_staff_attribute_type" catalog="openmrs">
        <id name="staffAttributeTypeId" type="int">
            <column name="staff_attribute_type_id" />
            <generator class="assigned" />
        </id>
        <property name="name" type="string">
            <column name="name" length="50" not-null="true" />
        </property>
        <property name="description" type="string">
            <column name="description" length="65535" not-null="true" />
        </property>
        <property name="handlerType" type="string">
            <column name="handler_type" length="50" not-null="true" />
        </property>
        <property name="handlerConfiguration" type="string">
            <column name="handler_configuration" length="50" />
        </property>
        <property name="minOccurs" type="int">
            <column name="min_occurs" not-null="true" />
        </property>
        <property name="maxOccurs" type="java.lang.Integer">
            <column name="max_occurs" />
        </property>
        <property name="creator" type="int">
            <column name="creator" not-null="true" />
        </property>
        <property name="dateCreated" type="timestamp">
            <column name="date_created" length="19" not-null="true" />
        </property>
        <property name="changedBy" type="java.lang.Integer">
            <column name="changed_by" />
        </property>
        <property name="dateChanged" type="timestamp">
            <column name="date_changed" length="19" />
        </property>
        <property name="retired" type="short">
            <column name="retired" not-null="true" />
        </property>
        <property name="retiredBy" type="java.lang.Integer">
            <column name="retired_by" />
        </property>
        <property name="dateRetired" type="timestamp">
            <column name="date_retired" length="19" />
        </property>
        <property name="retireReason" type="string">
            <column name="retire_reason" />
        </property>
        <property name="editPrivilege" type="string">
            <column name="edit_privilege" />
        </property>
        <property name="uuid" type="string">
            <column name="uuid" length="38" not-null="true" unique="true" />
        </property>
        <property name="sortWeight" type="java.lang.Double">
            <column name="sort_weight" precision="22" scale="0" />
        </property>
        <set name="hrStaffAttributes" inverse="true">
            <key>
                <column name="staff_attribute_type_id" />
            </key>
            <one-to-many class="org.openmrs.module.hr.HrStaffAttribute" />
        </set>
    </class>
</hibernate-mapping>
