<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.openmrs.module.hr.HrLeave" table="hr_leave" catalog="openmrs">
        <id name="leaveId" type="int">
            <column name="leave_id" />
            <generator class="assigned" />
        </id>
        <many-to-one name="hrPostHistory" class="org.openmrs.module.hr.HrPostHistory" fetch="select">
            <column name="post_history_id" not-null="true" />
        </many-to-one>
        <property name="startDate" type="timestamp">
            <column name="start_date" length="19" not-null="true" />
        </property>
        <property name="endDate" type="timestamp">
            <column name="end_date" length="19" />
        </property>
        <property name="leaveType" type="string">
            <column name="leave_type" length="10" />
        </property>
        <property name="leaveTypeOther" type="string">
            <column name="leave_type_other" length="252" />
        </property>
        <property name="creator" type="int">
            <column name="creator" not-null="true" />
        </property>
        <property name="dateCreated" type="timestamp">
            <column name="date_created" length="19" not-null="true" />
        </property>
        <property name="changedBy" type="java.lang.Integer">
            <column name="changed_by" />
        </property>
        <property name="dateChanged" type="timestamp">
            <column name="date_changed" length="19" />
        </property>
        <property name="retired" type="short">
            <column name="retired" not-null="true" />
        </property>
        <property name="retiredBy" type="java.lang.Integer">
            <column name="retired_by" />
        </property>
        <property name="dateRetired" type="timestamp">
            <column name="date_retired" length="19" />
        </property>
        <property name="retireReason" type="string">
            <column name="retire_reason" />
        </property>
        <property name="uuid" type="string">
            <column name="uuid" length="38" not-null="true" unique="true" />
        </property>
    </class>
</hibernate-mapping>
