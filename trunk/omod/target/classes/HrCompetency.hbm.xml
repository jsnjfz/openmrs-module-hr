<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.openmrs.module.hr.HrCompetency" table="hr_competency" catalog="openmrs">
        <id name="competencyId" type="int">
            <column name="competency_id" />
            <generator class="assigned" />
        </id>
        <property name="nationalId" type="java.lang.Integer">
            <column name="national_id" />
        </property>
        <property name="name" type="string">
            <column name="name" not-null="true" />
        </property>
        <property name="subname" type="string">
            <column name="subname" not-null="true" />
        </property>
        <property name="levels" type="string">
            <column name="levels" not-null="true" />
        </property>
        <property name="creator" type="int">
            <column name="creator" not-null="true" />
        </property>
        <property name="dateCreated" type="timestamp">
            <column name="date_created" length="19" not-null="true" />
        </property>
        <property name="changedBy" type="java.lang.Integer">
            <column name="changed_by" />
        </property>
        <property name="dateChanged" type="timestamp">
            <column name="date_changed" length="19" />
        </property>
        <property name="retired" type="short">
            <column name="retired" not-null="true" />
        </property>
        <property name="retiredBy" type="java.lang.Integer">
            <column name="retired_by" />
        </property>
        <property name="dateRetired" type="timestamp">
            <column name="date_retired" length="19" />
        </property>
        <property name="retireReason" type="string">
            <column name="retire_reason" />
        </property>
        <property name="editPrivilege" type="string">
            <column name="edit_privilege" />
        </property>
        <property name="uuid" type="string">
            <column name="uuid" length="38" not-null="true" unique="true" />
        </property>
        <property name="sortWeight" type="java.lang.Double">
            <column name="sort_weight" precision="22" scale="0" />
        </property>
        <set name="hrEvaluations" inverse="true">
            <key>
                <column name="competency_id" not-null="true" />
            </key>
            <one-to-many class="org.openmrs.module.hr.HrEvaluation" />
        </set>
    </class>
</hibernate-mapping>
